{
  "plugins": ["prettier", "import"],
  "extends": ["plugin:prettier/recommended", "prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-debugger": "off",
    "no-console": 0,
    "class-methods-use-this": "off"
  }
}
